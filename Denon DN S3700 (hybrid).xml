<?xml version='1.0' encoding='utf-8'?>
<MixxxControllerPreset mixxxVersion="" schemaVersion="1">
    <info>
        <name>Denon DN-S3700 (Hybrid Mode)</name>
        <author>HoneyHazard</author>
        <description>Controller preset for Denon DN-S3700 turntable operating in hybrid mode (midi + build-in vinyl emulation)</description>
        <forums>TODO</forums>
    </info>
    <controller id="DN-S3700">
        <scriptfiles>
            <file functionprefix="DenonDNS3700" filename="Denon-DN-S3700-hybrid-scripts.js"/>
        </scriptfiles>
        <controls>
            <control>
                <group>[Master]</group>
                <key>DenonDNS3700.inboundSysex</key>
                <status>0xF0</status>
                <options>
                    <script-binding/>
                </options>
            </control>
            <control>
                <group>[Master]</group>
                <key>DenonDNS3700.parametersRotaryChanged</key>
                <status>0xB0</status>
                <midino>0x54</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
            <control>
                <group>[Master]</group>
                <key>DenonDNS3700.presetDataChanged</key>
                <status>0x80</status>
                <midino>0x52</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
            <control>
                <group>[Master]</group>
                <key>DenonDNS3700.cueButtonChanged</key>
                <status>0x90</status>
                <midino>0x42</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
            <control>
                <group>[Master]</group>
                <key>DenonDNS3700.cueButtonChanged</key>
                <status>0x80</status>
                <midino>0x42</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
            <control>
                <group>[Master]</group>
                <key>DenonDNS3700.playButtonChanged</key>
                <status>0x90</status>
                <midino>0x43</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
            <control>
                <group>[Master]</group>
                <key>DenonDNS3700.ejectButtonPressed</key>
                <status>0x90</status>
                <midino>0x01</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
            <control>
                <group>[Master]</group>
                <key>DenonDNS3700.parametersButtonPressed</key>
                <status>0x90</status>
                <midino>0x28</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>keylock</key>
                <status>0x90</status>
                <midino>0x06</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>keylock</key>
                <status>0x80</status>
                <midino>0x06</midino>
                <options>
                    <normal/>
                </options>
            </control>
        </controls>
        <outputs/>
    </controller>
</MixxxControllerPreset>
